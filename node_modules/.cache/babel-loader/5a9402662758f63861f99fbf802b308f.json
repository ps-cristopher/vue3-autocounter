his.message.action = 'reset';\n      this.message.amount = null;\n    },\n  },\n});\n</script>\n\n<template>\n  <div class=\"vue3-autocounter\">\n    <p>The counter was {{ changedBy }} to <b>{{ counter }}</b>.</p>\n    <button @click=\"increment\">\n      Click +1\n    </button>\n    <button @click=\"decrement\">\n      Click -1\n    </button>\n    <button @click=\"increment(5)\">\n      Click +5\n    </button>\n    <button @click=\"decrement(5)\">\n      Click -5\n    </button>\n    <button @click=\"reset\">\n      Reset\n    </button>\n  </div>\n</template>\n\n<style scoped>\n  .vue3-autocounter {\n    display: block;\n    width: 400px;\n    margin: 25px auto;\n    border: 1px solid #ccc;\n    background: #eaeaea;\n    text-align: center;\n    padding: 25px;\n  }\n  .vue3-autocounter p {\n    margin: 0 0 1em;\n  }\n</style>\n"],"sourceRoot":""}]}